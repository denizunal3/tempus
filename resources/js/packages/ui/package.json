{
    "name": "@tempus/ui",
    "version": "0.0.9",
    "description": "Package containing the tempus ui components",
    "main": "./dist/tempus-ui-lib.umd.cjs",
    "module": "./dist/tempus-ui-lib.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tempus-io/tempus.git",
        "directory": "resources/js/packages/ui"
    },
    "types": "./dist/packages/ui/src/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/packages/ui/src/index.d.ts",
                "default": "./dist/tempus-ui-lib.js"
            },
            "require": {
                "types": "./dist/packages/ui/src/index.d.ts",
                "default": "./dist/tempus-ui-lib.umd.cjs"
            }
        },
        "./style.css": "./dist/style.css"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && vue-tsc --emitDeclarationOnly",
        "types": "vue-tsc ",
        "preview": "vite preview"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "tempus",
        "timetracker",
        "timetracking",
        "api",
        "client"
    ],
    "type": "module",
    "author": "tempus",
    "license": "AGPL-3.0",
    "devDependencies": {
        "@types/node": "^22.4.1",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.14",
        "postcss-import": "^15.1.0",
        "postcss-nesting": "^12.1.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.1",
        "vite-plugin-dts": "^4.0.3"
    },
    "peerDependencies": {
        "@floating-ui/vue": "^1.1.4",
        "@heroicons/vue": "^2.1.5",
        "@vueuse/core": "^10.11.0",
        "@zodios/core": "^10.9.6",
        "dayjs": "^1.11.13",
        "parse-duration": "^1.1.0",
        "tailwind-merge": "^2.5.2",
        "tailwindcss": "^3.1.0",
        "vue": "^3.4.38",
        "vue-tsc": "^2.0.29"
    }
}
